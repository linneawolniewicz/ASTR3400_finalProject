    "# Plot temp vs. radius\n",
    "plt.plot(logR, logT)\n",
    "plt.ylabel('Log(Temperature) (K)')\n",
    "plt.xlabel('Log(Radius) (Solar Radii)')\n",
    "plt.xlim(-5, 3)\n",
    "plt.title('Temperature versus Radius')"
