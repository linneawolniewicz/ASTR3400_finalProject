    "# Imports\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from astropy.io import ascii\n",
    "\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Variables\n",
    "filename = \"profile3.data\"\n",
    "datastart = 5"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Part 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Function that reads in data from filename and returns column1, column2, and column3 data\n",
    "def read_data(filename, datastart, column1, column2, column3):\n",
    "    # Read in data\n",
    "    data = ascii.read(filename, data_start = datastart)\n",
    "    \n",
    "    # Get temperature and radius data\n",
    "    column1_data = data['col' + column1]\n",
    "    column2_data = data['col' + column2]\n",
    "    column3_data = data['col' + column3]\n",
    "    \n",
    "    return column1_data, column2_data, column3_data"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Part 3 and 4"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Call function to get radius, temperature, and density data\n",
    "logR, logT, logD = read_data(filename, datastart, '7', '2', '3')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Temperature versus Radius"
   ]
  },
  {
   "cell_type": "code",
